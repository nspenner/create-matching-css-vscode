{
  "name": "create-matching-css-file",
  "displayName": "Create Matching CSS File",
  "description": "Creates a .css file based on current open editor name",
  "publisher": "NathanMinchow",
  "keywords": [
    "css", "quick", "create", "add", "match", "file"
  ],
  "icon": "icon.png",
  "repository": {
    "type": "github",
    "url": "https://github.com/nspenner/create-matching-css-vscode"
  },
  "version": "0.5.0",
  "engines": {
    "vscode": "^1.41.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onCommand:extension.createCSS",
    "onCommand:extension.createCSSContextMenu"
  ],
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "extension.createCSS",
        "title": "Create Matching .CSS File"
      },
      {
        "command": "extension.createCSSContextMenu",
        "title": "Create Matching .CSS File"
      }
    ],
    "menus": {
      "explorer/context": [
        {
          "command": "extension.createCSSContextMenu",
          "group": "navigation"
        }
      ]
    }
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile",
    "test": "node ./out/test/runTest.js"
  },
  "devDependencies": {
    "@types/glob": "^7.1.1",
    "@types/mocha": "^5.2.7",
    "@types/node": "^12.11.7",
    "@types/vscode": "^1.41.0",
    "glob": "^7.1.5",
    "mocha": "^6.2.2",
    "typescript": "^3.6.4",
    "tslint": "^5.20.0",
    "vscode-test": "^1.2.2"
  }
}
